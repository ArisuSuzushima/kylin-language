func add(x,y) public
    // Add 'x' and 'y'
    return x + y
end_func

func multiply(x,y) public
    // multiply
    return x * y
end_func

func subtract(x,y) public
    // subtract
    return x - y
end_func

func division(x,y) public
    return x / y
end_func

func eval(expression) public
    return expression
end_func